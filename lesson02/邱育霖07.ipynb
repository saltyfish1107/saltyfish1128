    "import csv\n",
    "f=[]\n",
    "with open('student.csv',encoding='utf-8') as file:\n",
    "    csvReader=csv.DictReader(file)\n",
    "    for row in csvReader:\n",
    "        print(row)\n",
    "        if int(row['age'])>25:\n",
    "            f.append(row)\n",
    "print('\\n篩選結果')\n",
    "for student in f:\n",
    "    print(student)"
